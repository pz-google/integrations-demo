{
  "id": "nginx",
  "displayName": "Nginx",
  "description": "Nginx, stylized as NGINX, nginx or NginX, is a web server that can also be used as a reverse proxy, load balancer, mail proxy and HTTP cache. The software was created by Igor Sysoev and publicly released in 2004. Nginx is free and open-source software, released under the terms of the 2-clause BSD license.",

  // The integration could integrated with either GCE or GKE
  "platforms": [
    "GCE", "GKE",
  ],
  "agentRequirement": {
    "minimumSupportedVersion": "2.1.0",
  },
  "detections": [
    // For GCE installation's detection
    {
      "pipelineType": "METRIC",
      "platform": "GCE",
      "condition": "workload.googleapis.com/nginx.requests",
    },

    {
      "pipelineType": "LOG",
      "platform": "GCE",
      "condition": "nginx",
    },

    // For GKE installation's detection
    // The metric is for demo purpose.
    {
      "pipelineType": "METRIC",
      "platform": "GKE",
      "condition": "workload.googleapis.com/kubernete/nginx.requests",
    },

  ],

  // Extra documentations could be provided as list
  // Such documentation in Google Cloud
  "documentationUrls": [
    {
      "label": "Google Cloud Ops Agent - nginx",
      "url": "https://cloud.google.com/monitoring/agent/ops-agent/third-party/nginx",
    }
  ],

  "launchStage": "GA",
}
